{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "definitions": {
    "destructuring_type_config": {
      "type": "object",
      "properties": {
        "array": {
          "type": "boolean"
        },
        "object": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    },
    "prefer_destructuring_options": {
      "type": "object",
      "properties": {
        "array": {
          "type": "boolean"
        },
        "object": {
          "type": "boolean"
        },
        "AssignmentExpression": {
          "$ref": "#/definitions/destructuring_type_config"
        },
        "VariableDeclarator": {
          "$ref": "#/definitions/destructuring_type_config"
        },
        "enforceForDeclarationWithTypeAnnotation": {
          "type": "boolean",
          "default": false
        },
        "enforceForRenamedProperties": {
          "type": "boolean",
          "default": false
        }
      },
      "additionalProperties": false
    }
  }
}
