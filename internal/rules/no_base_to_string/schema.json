{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "definitions": {
    "no_base_to_string_options": {
      "type": "object",
      "properties": {
        "checkUnknown": {
          "type": "boolean",
          "description": "Whether to also check values of type `unknown`.",
          "default": false
        },
        "ignoredTypeNames": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": ["Error", "RegExp", "URL", "URLSearchParams"]
        }
      }
    }
  }
}
